<mapper namespace="com.mapper.UserMapper">
	<select id="selectPic" resultType="com.pojo.User_table">
		select * from pic where name = #{name};
			</select>
		<select id="insertPic" resultType="com.pojo.User_table">
			insert into pic (name.url) values (#{name},#{url})   ;
		</select>
</mapper>